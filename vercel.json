{
  "builds": [
    {
      "src": "contract_generator/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9"
      }
    },
    {
      "src": "manage.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9"
      },
      "buildCommand": "python3 manage.py migrate && python3 manage.py collectstatic --noinput"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "contract_generator/wsgi.py"
    }
  ]
}